printBitmapAsCode :: proc(using bitmap: ^Bitmap) {
	using fmt;
	
	println("{");
	println("    width  = ", width,",");	
	println("    height = ", height,",");
	println("    pixels = {");
	
	for row in &pixels {
		print("        {");
		for pixel in &row {
			print("{");
			print("color={");
			printf("R=%3d, ", pixel.color.R);
			printf("G=%3d, ", pixel.color.G);
			printf("B=%3d", pixel.color.B);
			print("}, ");
			printf("opacity=%3d", pixel.opacity);
			print("}, ");
		}
		println("        },");
	}
	println("    }");
	println("};");
}

